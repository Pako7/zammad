<p id="notice"><%= notice %></p>

<h1>Tenants</h1>

<table>
  <thead>
    <tr>
      <th>Responsible</th>
      <th>Subdomain</th>
      <th>Company name</th>
      <th>Comercial name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Is active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenants.each do |tenant| %>
      <tr>
        <td><%= tenant.responsible %></td>
        <td><%= tenant.subdomain %></td>
        <td><%= tenant.company_name %></td>
        <td><%= tenant.comercial_name %></td>
        <td><%= tenant.phone %></td>
        <td><%= tenant.email %></td>
        <td><%= tenant.is_active %></td>
        <td><%= link_to 'Show', tenant %></td>
        <td><%= link_to 'Edit', edit_tenant_path(tenant) %></td>
        <td><%= link_to 'Destroy', tenant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenant', new_tenant_path %>
